@{
    ViewBag.Title = "Index";
    Layout = null;
}

<div id="divIndex" class="container-fluid">

    <div class="row">
        <div id="divCustomerDataView" class="col-md-3 column-layout"></div>
        <div id="containerPlanMigration" class="col-md-9 column-layout remove-padding">            
                <div id="" class="column-margin-layout bg-container">
                    <div class="row">
                        <div class="transaction-container-step">
                            <div class="transaction-process-step">
                                <div class="process-row-step nav nav-tabs">                       
                                </div>
                            </div>
                            <div class="transaction-progress-Steps">
                                <div class="transaction-process transaction-bar-step">
                                    <div id="prog" class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100" style="width: 0%">
                                    </div>
                                </div>
                            </div>
                        </div>                       
                    </div>

                </div>
            <div id="transactionContent" class="tab-content">
            </div>    
            <div class="row">
                <div class="footer-info-sot"  style="display:none; position:absolute;  bottom:-40px;  right:0px;background-color: rgb(116, 125, 132); width: 100%; height: 56px; color: rgb(255, 255, 255); text-align: center; padding: 10px; font-size: 25px; border-radius: 0px 0px 20px 20px;left:8px;">

                </div>
            </div>         
        </div>
    </div>

    <input type="hidden" id="hidFlagCampaniaColab" value='@System.Configuration.ConfigurationManager.AppSettings["strCambioPlanProy140245"].ToString()' /> 
    <input type="hidden" id="hidCodCampaniaColab" value='@System.Configuration.ConfigurationManager.AppSettings["strCodCampaniaColab"].ToString()' />
    <input type="hidden" id="hidMsgErrorConsultCam" value='@System.Configuration.ConfigurationManager.AppSettings["strMsgErrorConsultCam"].ToString()' />
    
</div>
@Scripts.Render("~/bundles/Content/Lib/BloqueoF12")
<script src="~/Areas/PlanMigration/Scripts/Index.js"></script>